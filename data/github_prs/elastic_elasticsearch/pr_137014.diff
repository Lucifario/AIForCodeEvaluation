 % TESTRESPONSE[s/]$/],"rewrite_time": $body.$_path, "collector": $body.$_path}], "aggregations": [], "fetch": $body.$_path}]}}/]
 % TESTRESPONSE[s/(?>=[" ])\d+(\.\d+)?/$body.$_path/]
